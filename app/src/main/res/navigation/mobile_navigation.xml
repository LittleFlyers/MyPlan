<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_calender">

    <fragment
        android:id="@+id/navigation_calender"
        android:name="com.zpf.myplan.ui.calendar.CalendarFragment"
        android:label="@string/title_plan"
        tools:layout="@layout/fragment_plan" />

    <fragment
        android:id="@+id/navigation_note"
        android:name="com.zpf.myplan.ui.note.NoteFragment"
        android:label="@string/title_plan"
        tools:layout="@layout/fragment_note" />

    <fragment
        android:id="@+id/navigation_event"
        android:name="com.zpf.myplan.ui.event.EventFragment"
        android:label="@string/title_event"
        tools:layout="@layout/fragment_event" />

    <fragment
        android:id="@+id/navigation_time"
        android:name="com.zpf.myplan.ui.time.TimeFragment"
        android:label="@string/title_plan"
        tools:layout="@layout/fragment_time" />

    <fragment
        android:id="@+id/navigation_mine"
        android:name="com.zpf.myplan.ui.mine.MineFragment"
        android:label="@string/title_mine"
        tools:layout="@layout/fragment_mine" />
</navigation>